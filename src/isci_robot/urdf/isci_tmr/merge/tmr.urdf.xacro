        <inertial>
            <mass value="1.0" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.00110833289" ixy="0.0" ixz="0.0" iyy="0.00110833289" iyz="0.0" izz="0.0018" />
        </inertial>
    </link>
    <joint name="shoulder_1_joint" type="revolute">
        <parent link="link_0" />
        <child link="link_1" />
        <origin rpy="0 0 0" xyz="0 0 0.1452" />
        <axis xyz="0 0 1" />
        <!--limit-->
        <limit effort="320.0" lower="-4.71238898038" upper="4.71238898038" velocity="3.14" />
    </joint>
    <link name="link_1">
        <visual>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/visual/tmr_400w_01.obj" />
            </geometry>
            <!-- <material name="link_1" /> -->
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/collision/tmr_400w_01_c.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="4.5" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.012487495005" ixy="0.0" ixz="0.0" iyy="0.012487495005" iyz="0.0" izz="0.0081" />
        </inertial>
    </link>
    <joint name="shoulder_2_joint" type="revolute">
        <parent link="link_1" />
        <child link="link_2" />
        <origin rpy="-1.57079632679 -1.57079632679 0" xyz="0 0 0" />
        <axis xyz="0 0 1" />
        <!--limit-->
        <limit effort="320" lower="-3.14159265359" upper="3.14159265359" velocity="3.14" />
    </joint>
    <link name="link_2">
        <visual>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/visual/tm5-900_arm1.obj" />
            </geometry>
            <!-- <material name="link_2" /> -->
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/collision/tm5-900_arm1_c.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="11.0" />
            <origin rpy="0 0 0" xyz="0.2145 0 -0.146" />
            <inertia ixx="0.0198" ixy="0.0" ixz="0.0" iyy="0.178604178558" iyz="0.0" izz="0.178604178558" />
        </inertial>
    </link>
    <joint name="elbow_joint" type="revolute">
        <parent link="link_2" />
        <child link="link_3" />
        <origin rpy="0 0 0" xyz="0.429 0 0" />
        <axis xyz="0 0 1" />
        <!--limit-->
        <limit effort="320" lower="-3.14159265359" upper="3.14159265359" velocity="3.14" />
    </joint>
    <link name="link_3">
        <visual>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/visual/tm5-900_arm2.obj" />
            </geometry>
            <!-- <material name="link_3" /> -->
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/collision/tm5-900_arm2_c.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="2.5" />
            <origin rpy="0 0 0" xyz="0.20575 0 -0.0163" />
            <inertia ixx="0.00253125" ixy="0.0" ixz="0.0" iyy="0.0365431624661" iyz="0.0" izz="0.0365431624661" />
        </inertial>
    </link>
    <joint name="wrist_1_joint" type="revolute">
        <parent link="link_3" />
        <child link="link_4" />
        <origin rpy="0 0 1.57079632679" xyz="0.4115 0 -0.1223" />
        <axis xyz="0 0 1" />
        <!--limit-->
        <limit effort="80" lower="-3.14159265359" upper="3.14159265359" velocity="3.92" />
    </joint>
    <link name="link_4">
        <visual>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/visual/tmr_100w_01.obj" />
            </geometry>
            <!-- <material name="link_4" /> -->
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/collision/tmr_100w_01_c.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="1.45" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.00247406151038" ixy="0.0" ixz="0.0" iyy="0.001468125" iyz="0.0" izz="0.00247406151038" />
        </inertial>
    </link>
    <joint name="wrist_2_joint" type="revolute">
        <parent link="link_4" />
        <child link="link_5" />
        <origin rpy="1.57079632679 0 0" xyz="0 -0.106 0" />
        <axis xyz="0 0 1" />
        <!--limit-->
        <limit effort="80" lower="-3.14159265359" upper="3.14159265359" velocity="3.92" />
    </joint>
    <link name="link_5">
        <visual>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/visual/tmr_100w_02.obj" />
            </geometry>
            <!-- <material name="link_5" /> -->
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/collision/tmr_100w_02_c.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="1.45" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.00247406151038" ixy="0.0" ixz="0.0" iyy="0.001468125" iyz="0.0" izz="0.00247406151038" />
        </inertial>
    </link>
    <joint name="wrist_3_joint" type="revolute">
        <parent link="link_5" />
        <child link="link_6" />
        <origin rpy="1.57079632679 0 0" xyz="0 -0.11315 0" />
        <axis xyz="0 0 1" />
        <!--limit-->
        <limit effort="80" lower="-4.71238898038" upper="4.71238898038" velocity="3.92" />
    </joint>
    <link name="link_6">
        <visual>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/visual/tmr_ee.obj" />
            </geometry>
            <!-- <material name="link_6" /> -->
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/collision/tmr_ee_c.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="0.4" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.000322499871" ixy="0.0" ixz="0.0" iyy="0.000322499871" iyz="0.0" izz="0.000405" />
        </inertial>
    </link>

    <link name="mid_mount"/>
    <joint name="mid_mount_joint" type="fixed">
      <parent link="link_chassis"/>
      <child link="mid_mount"/>
      <origin rpy="0 0 0" xyz="-0.2 0.0 0.34"/>
    </joint>
    
    <joint name="arm_mount_joint" type="fixed">
      <parent link="mid_mount"/>
      <child link="base"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </joint>

    <link name="base" />
    <joint name="base_fixed_joint" type="fixed">
        <parent link="base" />
        <child link="link_0" />
        <origin rpy="0 0 0" xyz="0 0 0" />
    </joint>
    <link name="flange" />
    <joint name="flange_fixed_joint" type="fixed">
        <parent link="link_6" />
        <child link="flange" />
        <origin rpy="0 0 0" xyz="0 0 0" />
    </joint>
  
  <link name="tool0"/>
  <joint name="flange_link-tool0" type="fixed">
    <parent link="flange"/>
    <child link="tool0"/>
    <origin rpy="0.000000 0.000000 0.000000" xyz="0.000000 0.000000 0.000000"/>
  </joint>
  
  <!--########################## BASE TRANSMISSIONS ############################################-->
  <transmission name="left_wheel_trans" type="SimpleTransmission">
  <type>transmission_interface/SimpleTransmission</type>
  <actuator name="left_wheel_motor">
   <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
   <mechanicalReduction>1</mechanicalReduction>
  </actuator>
  <joint name="joint_left_wheel">
   <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
  </joint>
  </transmission>

  <transmission name="right_wheel_trans" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="right_wheel_motor">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="joint_right_wheel">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>
<!--################################ ARM TRANSIMISSIONS ######################################-->
  <transmission name="arm_base_trans" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="arm_base_motor">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="shoulder_1_joint">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    <dynamics damping="10" friction="0.1"/>
    </joint>
  </transmission>

  <transmission name="shoulder_trans" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="shoulder_motor">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="shoulder_2_joint">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <transmission name="bottom_wrist_trans" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="bottom_wrist_motor">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="elbow_joint">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <transmission name="elbow_trans" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="elbow_motor">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="wrist_1_joint">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <transmission name="top_wrist_trans" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="top_wrist_motor">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="wrist_2_joint">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <transmission name="top_wrist_3_trans" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="top_wrist_motor2">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="wrist_3_joint">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>
<!--######################################################################-->
  <gazebo>
    
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">/
      <legacyModeNS>true</legacyModeNS>
      <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
    </plugin>

    <!-- <plugin name="joint_state_publisher" filename="libgazebo_ros_joint_state_publisher.so">
      <jointName>shoulder_1_joint, shoulder_2_joint, elbow_joint, wrist_1_joint, wrist_2_joint,wrist_3_joint</jointName>
    </plugin> -->

    <!-- <plugin name="gazebo_ros_control_select_joints" filename="libgazebo_ros_control_select_joints.so">
      <legacyModeNS>true</legacyModeNS>
      <jointName>shoulder_1_joint, shoulder_2_joint, elbow_joint, wrist_1_joint, wrist_2_joint,wrist_3_joint</jointName>
      <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
    </plugin> -->
  </gazebo>

<!--######################################################################-->
  <gazebo>
    <plugin name="diff_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>100.0</updateRate>
      <leftJoint>joint_right_wheel</leftJoint>
      <rightJoint>joint_left_wheel</rightJoint>
      <wheelSeparation>0.5</wheelSeparation>
      <wheelDiameter>0.16</wheelDiameter>
      <publishTf>true</publishTf>
      <odometryFrame>odom</odometryFrame>
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <robotBaseFrame>base_link</robotBaseFrame>
      <wheelAcceleration>3.0</wheelAcceleration>
      <wheelTorque>30</wheelTorque>
      <publishWheelJointState>false</publishWheelJointState>
      <publishWheelTF>false</publishWheelTF>
      <odometrySource>world</odometrySource>
      <rosDebugLevel>Debug</rosDebugLevel>
      <publishOdomTF>true</publishOdomTF>
    </plugin>
  </gazebo>
<!--######################################################################-->

  <gazebo reference="link_chassis">
    <material>Gazebo/Orange</material>
  </gazebo>
  <gazebo reference="link_left_wheel">
    <material>Gazebo/Blue</material>
  </gazebo>
  <gazebo reference="link_right_wheel">
    <material>Gazebo/Blue</material>
  </gazebo>

  <gazebo reference="caster_front">
    <mu1>0.00001</mu1>
    <mu2>0.00001</mu2>
    <material>Gazebo/White</material>
  </gazebo>
  <gazebo reference="caster_back">
    <mu1>0.00001</mu1>
    <mu2>0.00001</mu2>
    <material>Gazebo/White</material>
  </gazebo>

  <gazebo reference="link_1">
