<?xml version="1.0" ?>
<robot name="isci_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
  
  <link name="base_link" /> 
    
  <link name="link_chassis">
    <!-- pose and inertial -->
    <inertial>
      <mass value="50"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia
        ixx="0.774276574699151"
        ixy="-1.03781944357671E-10"
        ixz="0.00763014265820928"
        iyy="1.64933255189991"
        iyz="1.09578155845563E-12"
        izz="2.1239326987473" />
    </inertial>
    <!-- body -->
    <!-- <collision name="collision_chassis">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.8 0.5 0.68"/>
      </geometry>
    </collision> -->
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.8 0.5 0.68"/>
      </geometry>
      <material name="">
          <color rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>   
  </link>

  <joint name="base_link_joint" type="fixed">
    <parent link="base_link"/>
    <child link="link_chassis" />
    <origin xyz="0 0 0.51" rpy="0 0 0"/>
  </joint>
  
  <link name="caster_front">
    <!-- pose and inertial -->
    <inertial>
      <mass value="1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia
        ixx="0.00856502765719703"
        ixy="1.5074118157338E-19"
        ixz="-4.78150098725052E-19"
        iyy="0.013670640432096"
        iyz="-2.68136447099727E-19"
        izz="0.00856502765719703" />
    </inertial>
    <collision name="caster_front_collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.08"/>
      </geometry>
    </collision>
    <visual name="caster_front_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.08"/>
      </geometry>
    </visual>
  </link>

  <joint name="bottom2caster_front_joint" type="fixed">
    <parent link="link_chassis"/>
    <child link="caster_front" />
    <origin xyz="0.3 0 -0.43" rpy="0 0 0"/>
  </joint>

  <link name="caster_back">
    <!-- pose and inertial -->
    <inertial>
      <mass value="1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia
        ixx="0.00856502765719703"
        ixy="1.5074118157338E-19"
        ixz="-4.78150098725052E-19"
        iyy="0.013670640432096"
        iyz="-2.68136447099727E-19"
        izz="0.00856502765719703" />
    </inertial>
    <collision name="caster_back_collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.08"/>
      </geometry>
    </collision>
    <visual name="caster_back_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.08"/>
      </geometry>
    </visual>
  </link>

  <joint name="bottom2caster_back_joint" type="fixed">
    <parent link="link_chassis"/>
    <child link="caster_back" />
    <origin xyz="-0.3 0 -0.43" rpy="0 0 0"/>
  </joint>

  <link name="link_right_wheel">
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2.6578" />
      <inertia
        ixx="0.00856502765719703"
        ixy="1.5074118157338E-19"
        ixz="-4.78150098725052E-19"
        iyy="0.013670640432096"
        iyz="-2.68136447099727E-19"
        izz="0.00856502765719703" />
      </inertial>
    <collision name="link_right_wheel_collision">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
    </collision>
    <visual name="link_right_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
  </link>
  
  <joint name="joint_right_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0 0.25 -0.43"/>
    <child link="link_right_wheel"/>
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>

    <dynamics damping="0.5"/>
    <limit effort="10" velocity="20"/>
  </joint>
  
  <link name="link_left_wheel">
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2.6578" />
      <inertia
        ixx="0.00856502765719703"
        ixy="1.5074118157338E-19"
        ixz="-4.78150098725052E-19"
        iyy="0.013670640432096"
        iyz="-2.68136447099727E-19"
        izz="0.00856502765719703" />
      </inertial>
    <collision name="link_left_wheel_collision">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
    </collision>
    <visual name="link_left_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
      <material name="LightGrey">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>
  
  <joint name="joint_left_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0 -0.25 -0.43"/>
    <child link="link_left_wheel"/>
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>

    <dynamics damping="0.5"/>
    <limit effort="10" velocity="20"/>
  </joint>


  <!--******************* add Velodyne Link ********************************-->
  <link name="VLP16_base_link">
  </link>

  <joint name="VLP16_base_mount_joint" type="fixed">
    <origin xyz="0.0 0 0.3823" rpy="0 -0.17444 0"/>
    <child link="VLP16_base_link"/>
    <parent link="link_chassis"/>
  </joint>   

  <xacro:include filename="$(find velodyne_description)/urdf/VLP-16.urdf.xacro"/>
  <xacro:VLP-16 parent="VLP16_base_link" name="velodyne" topic="/velodyne_points" hz="10" samples="1000" >
    <origin xyz="0 0.0 0.0" rpy="0 0 0" />
  </xacro:VLP-16>

  <!--******************* add TM5 Link *************************************-->
  <link name="link_0">
        <visual>
            <geometry>
                <mesh filename="package://isci_robot/urdf/isci_tmr/meshes/tm5-900/visual/tm5_base.obj" />
